    <table class="table table-striped">
      <thead>
        <tr>
          <th>Nome da startup</th>
          <th>Website</th>
          <th>Endere√ßo</th>
          <th>Link no SPV</th>
        </tr>
      </thead>

      <tbody>
        <% @companies.each do |company| %>
        <tr>
          <td><%= company.title %></td>
          <td><%= company.website %></td>
          <td><%= company.address %></td>
          <td><%= company.link %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

<%= paginate @companies %>
